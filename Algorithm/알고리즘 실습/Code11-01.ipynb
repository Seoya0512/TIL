    "def findMinIndex(ary):\n",
    "    minIndex = 0\n",
    "    for i in range(1, len(ary)):\n",
    "        if (ary[minIndex] > ary[i]):\n",
    "            minIndex = i\n",
    "    return minIndex\n"
   ],
   "outputs": [],
   "metadata": {}
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "source": [
    "testAry = [55, 88, 33, 77]\n",
    "minPos = findMinIndex(testAry)\n",
    "print('최솟값 --->', testAry[minPos])"
   ],
